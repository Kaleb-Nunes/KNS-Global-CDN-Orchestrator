worker_processes auto;
worker_rlimit_nofile 65535;
events { worker_connections 16384; }
http { sendfile on; tcp_nopush on; directio 512; aio on; proxy_cache_path /var/cache/nginx levels=1:2 keys_zone=cdn_cache:10m max_size=10g inactive=60m; server { listen 80; location / { proxy_cache cdn_cache; proxy_pass http://origem; add_header X-Cache-Status $upstream_cache_status; } } }